{
  "version": 2,
  "buildCommand": "npm run prisma:generate && npm run build",
  "outputDirectory": "dist",
  "build": {
    "env": {
      "TS_NODE_TRANSPILE_ONLY": "true"
    }
  },
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ]
}